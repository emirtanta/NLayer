@using NLayer.EntityLayer.Concrete
@model List<About>
@{
    ViewBag.Title = "Index";
}

<h2>Hakkımda Veri Listesi</h2>



<br />

<table class="table table-hover table-responsive">
    <tr>
        <th>#</th>
        <th>Hakkımda Başlığı</th>
        <th>Aktif Yap</th>
        <th>Pasif Yap</th>
    </tr>
    @foreach (var item in Model)
    {
<tr>
    <td>@item.AboutID</td>
    <td>@item.AboutDetails1</td>
    <td>Başlık</td>
    <td>
        <a href="#" class="btn btn-warning">Aktif yap</a>
    </td>
    <td>
        <a href="#" class="btn btn-warning">Pasif yap</a>
    </td>
</tr>
       
    }
</table>


@Html.Action("AboutPartial", "About")